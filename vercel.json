{
  "builds": [
    {
      "src": "api/youtube-api.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/github.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/youtube-api",
      "dest": "/api/youtube-api.js"
    },
    {
      "src": "/api/github",
      "dest": "/api/github.js"
    }
  ],
  "env": {
    "YOUTUBE_API_KEY": "your_actual_youtube_api_key"
  }
}
